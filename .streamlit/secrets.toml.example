# Streamlit Secrets Example
# Copy this file to secrets.toml and fill in your actual values
# secrets.toml is gitignored for security

[database]
SUPABASE_URL = "https://your-project.supabase.co"
SUPABASE_ANON_KEY = "your_anon_key_here"
SUPABASE_SERVICE_KEY = "your_service_key_here"

[alpaca]
ALPACA_API_KEY = "your_alpaca_api_key_here"
ALPACA_SECRET_KEY = "your_alpaca_secret_key_here"
ALPACA_PAPER = "true"

[apis]
QUIVER_API_KEY = "your_quiver_api_key_here"

[trading]
TRADING_MIN_CONFIDENCE = "0.65"

[risk]
RISK_MAX_POSITION_SIZE_PCT = "10.0"
RISK_MAX_PORTFOLIO_RISK_PCT = "2.0"
RISK_MAX_TOTAL_EXPOSURE_PCT = "80.0"
RISK_MAX_POSITIONS = "20"
